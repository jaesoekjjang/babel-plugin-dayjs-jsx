    "@emotion/babel-plugin-jsx-pragmatic": "^0.2.1",
    "@types/react-dom": "^18.2.18",
    "babylon": "^6.18.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "^7.23.3",
